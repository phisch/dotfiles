{
  "assistant": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "claude-3-5-sonnet"
    },
    "version": "2"
  },
  "auto_update": false,
  "buffer_font_family": "JetBrains Mono",
  "buffer_font_size": 18,
  "buffer_font_weight": 200,
  "drop_target_size": 0.33,
  "features": {
    "inline_completion_provider": "copilot"
  },
  "gutter": {
    "folds": false
  },
  "message_editor": {
    "auto_replace_emoji_shortcode": true
  },
  "preview_tabs": {
    "enable_preview_from_code_navigation": true,
    "enable_preview_from_file_finder": true
  },
  "project_panel": {
    "auto_fold_dirs": false
  },
  "tabs": {
    "git_status": true
  },
  "telemetry": {
    "enabled": false,
    "metrics": false,
    "diagnostics": false
  },
  "terminal": {
    "font_weight": 200,
    "font_family": "JetBrains Mono",
    "font_size": 16,
    "shell": {
      "program": "/usr/bin/nu"
    },
    "toolbar": {
      "breadcrumbs": false
    }
  },
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "Phine"
  },
  "ui_font_size": 18,
  "ui_font_weight": 200,
  "unnecessary_code_fade": 0.8,
  "unstable.ui_density": "default",
  "use_smartcase_search": true,
  "wrap_guides": [80, 120]
}
